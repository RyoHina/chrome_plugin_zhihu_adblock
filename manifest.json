{
	"manifest_version": 2,
	"name": "屏蔽知乎广告",
	"version": "0.4",
	"description": "屏蔽知乎广告",
	"author": "kyle",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action":
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts":
	[
		{
			"matches": ["https://www.zhihu.com/*"],
			"js": ["content-script.js"],
			"run_at":"document_start"
		}
	],
	"web_accessible_resources":
	[
		"main.js",
		"jquery.min.js"
	]
}
